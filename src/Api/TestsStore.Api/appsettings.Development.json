{
	"ConnectionString": "Server=localhost\\SQLEXPRESS01;Database=TestsStore;Integrated Security=true",
	"Logging": {
		"LogLevel": {
			"Default": "Debug",
			"System": "Information",
			"Microsoft": "Information"
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Debug",
			"Override": {
				"System": "Information",
				"Microsoft": "Information"
			}
		},
		"WriteTo": [
			{ "Name": "Console" },
			{
				"Name": "File",
				"Args": {
					"path": "Log\\log.txt",
					"rollingInterval": "Day"
				}
			}
		],
		"Enrich": [ "FromLogContext" ]
	}
}
